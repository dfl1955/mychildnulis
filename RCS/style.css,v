/* body { background-color: #94ff94; } */

acronym { text-decoration: dotted;}

/* it'd be nice to underline the links witin a paragraph */

.entry-content a { text-decoration: underline; }
.comment-list a { text-decoration: underline;  }

/* a { color: #FFC600; } a:link	{ color: #FFC600; } a:visited { color: #B45F04; } */

.site-feeds ul  { list-style-type: none; }
.site-feeds li:after { content:url('http://davelevy.info/TOKENS/rss-feed-icon-16.png');}

/*-----------------------------------------------------*/

.iconized.a-icon { background-image: url('assets/images/downarrow.png');}
.iconized.r-icon { background-image: url('assets/images/rss1.png');}
.iconized.i-icon { background-image: url('assets/images/a-z.png');}
.iconized.q-icon { background-image: url('assets/images/quill.png');}

.arrow-icon:before {
    width: 48px, height 48px; text-align: center; font: 3.2rem/1.5 Genericons; vertical-align: middle; display: inline-block;
   -moz-osx-font-smoothing: grayscale; -webki-font-smoothing: antialiased;
    content: "\f419"; }

.rss-icon:before {
    width: 48px, height 48px; text-align: center; font: 3.2rem/1.5 Genericons; vertical-align: middle; display: inline-block;
   -moz-osx-font-smoothing: grayscale; -webki-font-smoothing: antialiased;
    content: "\f419"; }

.az-icon:before {
    width: 48px, height 48px; text-align: center; font: 3.2rem/1.5 Genericons; vertical-align: middle; display: inline-block;
   -moz-osx-font-smoothing: grayscale; -webki-font-smoothing: antialiased;
    content: "\f419"; }

.qu-icon:before {
    width: 48px, height 48px; text-align: center; font: 3.2rem/1.5 Genericons; vertical-align: middle; display: inline-block;
   -moz-osx-font-smoothing: grayscale; -webki-font-smoothing: antialiased;
    content: "\f419"; }

.arrow-icon { width: 48px; height:48px; display: inline-block;}
.rss-icon { width: 48px; height:48px; display: inline-block;}
.az-icon { width: 48px; height:48px; display: inline-block;}
.qu-icon { width: 48px; height:48px; display: inline-block;}

.arrow-icon:focus {border:0;}
.rss-icon:focus {border:0;}
.az-icon:focus {border:0;}
.qu-icon:focus {border:0;}

.arrow-icon-wrapper { float: left;}
.rss-icon-wrapper { float: left;}
.az-icon-wrapper { float: left;}
.qu-icon-wrapper { float: left;}

